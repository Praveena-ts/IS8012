@page
@model GroceryList.Pages.ItemLists.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ItemList[0].Billingdate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemList[0].ItemName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemList[0].Billamount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemList[0].IsBillingComplete)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ItemList) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Billingdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Billamount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsBillingComplete)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>


